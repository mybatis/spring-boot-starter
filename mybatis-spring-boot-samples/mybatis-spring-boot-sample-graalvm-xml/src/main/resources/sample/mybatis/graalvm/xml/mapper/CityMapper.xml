<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sample.mybatis.graalvm.xml.mapper.CityMapper">
    <select id="selectCityById" resultType="sample.mybatis.graalvm.xml.domain.City">
        select id, name, state, country from city where id = #{id}
    </select>
    <select id="findByState" resultType="sample.mybatis.graalvm.xml.domain.City">
        select id, name, state, country from city where state = #{state}
    </select>
    <select id="listByState" resultType="sample.mybatis.graalvm.xml.domain.City">
        select id, name, state, country from city where state = #{state}
    </select>
    <select id="mapByState" resultType="java.util.Map">
        select id, name, state, country from city where state = #{state}
    </select>
    <select id="listMapByState" resultType="java.util.Map">
        select id, name, state, country from city where state = #{state}
    </select>
    <select id="treeSetStateByState" resultType="java.lang.String">
        select state from city where country = #{country}
    </select>
    <select id="hashSetStateByState" resultType="java.lang.String">
        select state from city where country = #{country}
    </select>
</mapper>
